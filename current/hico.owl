#   Object Properties
############################

# Object Property: <http://purl.org/emmedi/hico/hasInterpretationCriterion> (has interpretation criterion)

AnnotationAssertion(rdfs:comment <http://purl.org/emmedi/hico/hasInterpretationCriterion> "A property relating an interpretation act to a criterion or type of source used to support the claim."@en)
AnnotationAssertion(rdfs:isDefinedBy <http://purl.org/emmedi/hico/hasInterpretationCriterion> <http://purl.org/emmedi/hico>)
AnnotationAssertion(rdfs:label <http://purl.org/emmedi/hico/hasInterpretationCriterion> "has interpretation criterion"@en)
ObjectPropertyDomain(<http://purl.org/emmedi/hico/hasInterpretationCriterion> <http://purl.org/emmedi/hico/InterpretationAct>)
ObjectPropertyRange(<http://purl.org/emmedi/hico/hasInterpretationCriterion> <http://purl.org/emmedi/hico/InterpretationCriterion>)

# Object Property: <http://purl.org/emmedi/hico/hasInterpretationType> (has interpretation type)

AnnotationAssertion(rdfs:comment <http://purl.org/emmedi/hico/hasInterpretationType> "A property relating an interpretation act to an individual defining an arbitrary classification of the interpretation."@en)
AnnotationAssertion(rdfs:isDefinedBy <http://purl.org/emmedi/hico/hasInterpretationType> <http://purl.org/emmedi/hico>)
AnnotationAssertion(rdfs:label <http://purl.org/emmedi/hico/hasInterpretationType> "has interpretation type"@en)
ObjectPropertyDomain(<http://purl.org/emmedi/hico/hasInterpretationType> <http://purl.org/emmedi/hico/InterpretationAct>)
ObjectPropertyRange(<http://purl.org/emmedi/hico/hasInterpretationType> <http://purl.org/emmedi/hico/InterpretationType>)

# Object Property: <http://purl.org/emmedi/hico/isExtractedFrom> (is extracted from)

AnnotationAssertion(rdfs:comment <http://purl.org/emmedi/hico/isExtractedFrom> "A property relating an interpretation act to the source where from the claim has been extracted."@en)
AnnotationAssertion(rdfs:isDefinedBy <http://purl.org/emmedi/hico/isExtractedFrom> <http://purl.org/emmedi/hico>)
AnnotationAssertion(rdfs:label <http://purl.org/emmedi/hico/isExtractedFrom> "is extracted from"@en)
SubObjectPropertyOf(<http://purl.org/emmedi/hico/isExtractedFrom> <http://www.w3.org/ns/prov#wasInfluencedBy>)
ObjectPropertyDomain(<http://purl.org/emmedi/hico/isExtractedFrom> <http://purl.org/emmedi/hico/InterpretationAct>)

